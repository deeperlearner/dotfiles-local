# Alias
# Variable
## Path
### virtualenvwrapper
export VIRTUALENVWRAPPER_PYTHON=/usr/bin/python3
export WORKON_HOME=$HOME/.virtualenvs
export PROJECT_HOME=$HOME/Dev
source ~/.local/bin/virtualenvwrapper.sh
### dir link
export DATA='/mnt/d'
export backup_path="$DATA/Git*Backup/ntu"
export download_path="$DATA/Download"
export git_path="$HOME/Github/myrepo"
export dot_path="$git_path/dotfiles"
export pytorch_template="$git_path/pytorch-template"
### torch
export TORCH_HOME="$download_path/models"
